{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "types": ["node", "hardhat", "chai"]
  },
  "include": ["./scripts", "./deploy", "./test", "./hardhat.config.ts"],
  "exclude": ["node_modules"]
}


